			CreateHITBOXEXon(plr)
			wait()
			game.Players.LocalPlayer.Character.HumanoidRootPart.Size = Vector3.new(2,2,1)
			wait(0.1)
			game.Players.LocalPlayer.Character.HumanoidRootPart.Size = Vector3.new(2,2,1)
		end
	end 
end

execute.MouseButton1Up:Connect(function()
	if game.CoreGui:FindFirstChild("Booga Booga - Hybrid [Daynight Hub]") == nil then
		ScreenGui.Enabled = false
		executeSCRIPT()
	end
end)
