function executeSCRIPT()
	local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/zxciaz/VenyxUI/main/Reuploaded"))() --someone reuploaded it so I put it in place of the original back up so guy can get free credit.
	local venyx = library.new("Booga Booga - Hybrid [Daynight Hub]", 5013109572)
	local notifications = true

	local themes = {
		Background = Color3.fromRGB(24, 24, 24),
		Glow = Color3.fromRGB(0, 0, 0),
		Accent = Color3.fromRGB(10, 10, 10),
		LightContrast = Color3.fromRGB(20, 20, 20),
		DarkContrast = Color3.fromRGB(14, 14, 14),  
		TextColor = Color3.fromRGB(255, 255, 255)
	}
	local main = venyx:addPage("Main", 5012544693)
	local mainSL = main:addSection("Thanks for using [Daynight Hub]")

	mainSL:addButton("Copy Discord Link", function()
		setclipboard("https://discord.gg/UMUp5tXgUU")
	end)
	local update = main:addSection("Version 1.02")
	update:addButton("added a gui for backup Settings/Destroy-Gui", function()

	end)
	update:addButton("added aether teleport", function()

	end)

	local playerTab = venyx:addPage("Player", 5012544693)
	local SpeedSL = playerTab:addSection("Speed")
	local flySL = playerTab:addSection("Fly")
	local autobreak = playerTab:addSection("AutoBreak")
	local Hip = playerTab:addSection("HipHeight")
	local FOV = playerTab:addSection("FieldOfView")

	local plr = game:GetService("Players").LocalPlayer
	local char = plr.Character
	local mouse = game:GetService("Players").LocalPlayer:GetMouse()
	local hum = char:FindFirstChild("HumanoidRootPart")
	local loop = false
	local loo3p = false
	local dotoggle = false
	local dotoggle2 = false
	local dotoggle3 = false
	local breaking = true
	local did = false
	local speed = 1
	local norheight = 2
	local backspeedlolidc = false

	SpeedSL:addToggle("Toggle Speed", false, function(value)
		if value == false then
			value = true
			dotoggle = false
			if notifications == true then
				venyx:Notify("Speed", "Off    ")
			end
		elseif value == true then
			value = false
			dotoggle = true
			if notifications == true then
				venyx:Notify("Speed", "On     ")
			end
		end
	end)

	SpeedSL:addToggle("Toggle Backwards Speed", false, function(value)
		if value == false then
			value = true
			backspeedlolidc = false
			if notifications == true then
				venyx:Notify("Backwards Speed", "Off                ")
			end
		elseif value == true then
			value = false
			backspeedlolidc = true
			if notifications == true then
				venyx:Notify("Backwards Speed", "On                 ")
			end
		end
	end)

	SpeedSL:addSlider("Change Speed", 1, 0, 3, function(value)
		speed = value
	end)

	SpeedSL:addButton("See Your Speed", function()
		venyx:Notify("Speed", speed.."    ")
	end)
	flySL:addButton("Go on a Raft OR Activate Speed", function()

	end)
	
	local maxspeed = 50

	flySL:addButton("Fly [E]", function()
		local speed55 = 0 
		repeat wait() 
		until game.Players.LocalPlayer and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:findFirstChild("Head") and game.Players.LocalPlayer.Character:findFirstChild("Humanoid") 
		local mouse = game.Players.LocalPlayer:GetMouse() 
		repeat wait() until mouse
		local plr = game.Players.LocalPlayer 
		local torso = plr.Character.Head 
		local flying = false
		local deb = true 
		local ctrl = {f = 0, b = 0, l = 0, r = 0} 
		local lastctrl = {f = 0, b = 0, l = 0, r = 0} 

		function Fly() 
			local bg = Instance.new("BodyGyro", torso) 
			bg.P = 9e4 
			bg.maxTorque = Vector3.new(9e9, 9e9, 9e9) 
			bg.cframe = torso.CFrame 
			local bv = Instance.new("BodyVelocity", torso) 
			bv.velocity = Vector3.new(0,0.1,0) 
			bv.maxForce = Vector3.new(9e9, 9e9, 9e9) 
			repeat wait() 
				plr.Character.Humanoid.PlatformStand = true 
				if ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0 then 
					speed55 = speed55+.5+(speed55/maxspeed) 
					if speed55 > maxspeed then 
						speed55 = maxspeed 
					end 
				elseif not (ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0) and speed55 ~= 0 then 
					speed55 = speed55-1 
					if speed55 < 0 then 
						speed55 = 0 
					end 
				end 
				if (ctrl.l + ctrl.r) ~= 0 or (ctrl.f + ctrl.b) ~= 0 then 
					bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f+ctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(ctrl.l+ctrl.r,(ctrl.f+ctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed55 
					lastctrl = {f = ctrl.f, b = ctrl.b, l = ctrl.l, r = ctrl.r} 
				elseif (ctrl.l + ctrl.r) == 0 and (ctrl.f + ctrl.b) == 0 and speed55 ~= 0 then 
					bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (lastctrl.f+lastctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(lastctrl.l+lastctrl.r,(lastctrl.f+lastctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed55 
				else 
					bv.velocity = Vector3.new(0,0.1,0) 
				end 
				bg.cframe = game.Workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(-math.rad((ctrl.f+ctrl.b)*50*speed55/maxspeed),0,0) 
			until not flying 
			ctrl = {f = 0, b = 0, l = 0, r = 0} 
			lastctrl = {f = 0, b = 0, l = 0, r = 0} 
			speed55 = 0 
			bg:Destroy() 
			bv:Destroy() 
			plr.Character.Humanoid.PlatformStand = false 
		end 
		mouse.KeyDown:connect(function(key) 
			if key:lower() == "e" then 
				if flying then flying = false 
				else 
					flying = true 
					Fly() 
				end 
			elseif key:lower() == "w" then 
				ctrl.f = 1 
			elseif key:lower() == "s" then 
				ctrl.b = -1 
			elseif key:lower() == "a" then 
				ctrl.l = -1 
			elseif key:lower() == "d" then 
				ctrl.r = 1 
			end 
		end) 
		mouse.KeyUp:connect(function(key) 
			if key:lower() == "w" then 
				ctrl.f = 0 
			elseif key:lower() == "s" then 
				ctrl.b = 0 
			elseif key:lower() == "a" then 
				ctrl.l = 0 
			elseif key:lower() == "d" then 
				ctrl.r = 0 
			end 
		end)
		Fly()
	end)
	
	SpeedSL:addSlider("Change Speed", 1, 0, 100, function(value)
		maxspeed = value
	end)


	local UIS = game:GetService("UserInputService")
	UIS.InputBegan:Connect(function(input)
		if input.KeyCode == Enum.KeyCode.W and dotoggle == true then
			if dotoggle == true then
				loo3p = true
				while loo3p do
					hum.CFrame = hum.CFrame + hum.CFrame.lookVector * speed
					wait()
				end
			end
		end
	end)

	UIS.InputEnded:Connect(function(output)
		if output.KeyCode == Enum.KeyCode.W then
			if dotoggle == true then
				loo3p = false
			end
		end
	end)

	local loopidc = false

	UIS.InputBegan:Connect(function(input)
		if input.KeyCode == Enum.KeyCode.S and dotoggle == true then
			if backspeedlolidc == true then
				loopidc = true
				while loopidc do
					hum.CFrame = hum.CFrame + hum.CFrame.lookVector * -1
					wait()
				end
			end
		end
	end)

	UIS.InputEnded:Connect(function(output)
		if output.KeyCode == Enum.KeyCode.S then
			if backspeedlolidc == true then
				loopidc = false
			end
		end
	end)

	autobreak:addToggle("Toggle Auto Break", false, function(value)
		if value == false then
			value = true
			dotoggle2 = true
			if notifications == true then
				venyx:Notify("Auto Break", "On     ")
			end
			breaking = false
		elseif value == true then
			value = false
			dotoggle2 = true
			if notifications == true then
				venyx:Notify("Auto Break", "Off      ")
			end
			breaking = true
			while breaking and wait(0.1) do
				local part = game:GetService("Players").LocalPlayer:GetMouse().Target
				local one = game:GetService("ReplicatedStorage").RelativeTime.Value
				local two = {part,part,part,part}
				game:GetService("ReplicatedStorage").Events.SwingTool:FireServer(one, two)
			end
		end
	end)

	Hip:addSlider("HipHeight", 2, 0, 100, function(value)
		game.Players.LocalPlayer.Character.Humanoid.HipHeight = value
		norheight = value
	end)
	Hip:addButton("Normal HipHeight", function()
		game.Players.LocalPlayer.Character.Humanoid.HipHeight = 2
	end)

	FOV:addSlider("FieldOfView", 70, 10, 120, function(value)
		game.Workspace.CurrentCamera.FieldOfView = value
	end)

	local FunYAY = playerTab:addSection("Fun")
	FunYAY:addButton("Dex Explorer V2", function()
		loadstring(game:HttpGet("https://cdn.wearedevs.net/scripts/Dex Explorer V2.txt"))()
	end)
	FunYAY:addButton("Sit Down", function()
		game.Players.LocalPlayer.Character.Humanoid.Sit = true
	end)

	local autoESP = false
	local autoHITBOX = false

	local hitbox = playerTab:addSection("Extend Hitbox")
	Important = {Players = game:GetService("Players"), Workspace = game:GetService("Workspace"), CoreGui = game:GetService("CoreGui")}
	
	function CreateHITBOXEXoff(plr)
		if plr ~= nil then
			local GetChar = plr.Character
			if not GetChar then return end

			local GetHead do
				repeat wait() until GetChar:FindFirstChild("HumanoidRootPart")
			end
			GetHead = GetChar.HumanoidRootPart     
			GetHead.Size = Vector3.new(2, 2, 1)
		end
	end

	function CreateHITBOXEXon(plr)
		if plr ~= nil then
			local GetChar = plr.Character
			if not GetChar then return end

			local GetHead do
				repeat wait() until GetChar:FindFirstChild("HumanoidRootPart")
			end
			GetHead = GetChar.HumanoidRootPart
			GetHead.Size = Vector3.new(10, 10, 10)
		end
	end

	hitbox:addToggle("Auto Hitbox", false, function(value)
		if value == false then
			value = true
			autoHITBOX = false
			if notifications == true then
				venyx:Notify("Auto Hitbox", "Off    ")
			end
		elseif value == true then
			value = false
			autoHITBOX = true
			if notifications == true then
				venyx:Notify("Auto Hitbox", "On     ")
			end
		end
	end)

	hitbox:addButton("Extend Hitbox On", function()
		for i,v in pairs(Important.Players:GetChildren()) do
			CreateHITBOXEXon(v) 
			wait()
			game.Players.LocalPlayer.Character.HumanoidRootPart.Size = Vector3.new(2,2,1)
		end
	end)

	hitbox:addButton("Extend Hitbox Off", function()
		for i,v in pairs(Important.Players:GetChildren()) do
			CreateHITBOXEXoff(v) 
			wait()
			game.Players.LocalPlayer.Character.HumanoidRootPart.Size = Vector3.new(2,2,1)
		end
	end)

	local Visual = playerTab:addSection("Visual")
	Important = {Players = game:GetService("Players"), Workspace = game:GetService("Workspace"), CoreGui = game:GetService("CoreGui")}
	local enabledesp = false

	function CreateESP(plr)
		if plr ~= nil then

			local GetChar = plr.Character
			if not GetChar then return end

			local GetHead do

				repeat wait() until GetChar:FindFirstChild("Head")

			end
			GetHead = GetChar.Head        

			local bb = Instance.new("BillboardGui", Important.CoreGui)
			bb.Adornee = GetHead
			bb.ExtentsOffset = Vector3.new(0, 1, 0)
			bb.AlwaysOnTop = true
			bb.Size = UDim2.new(0, 5, 0, 5)
			bb.StudsOffset = Vector3.new(0, 3, 0)
			bb.Name = "ESP_PLAYER_"

			local displayframe = Instance.new("Frame", bb)
			displayframe.ZIndex = 10
			displayframe.BackgroundTransparency = 1
			displayframe.Size = UDim2.new(1,0,1,0)

			local name = Instance.new("TextLabel", displayframe)
			name.Name = "Name"
			name.ZIndex = 10
			name.Text = plr.Name
			name.Visible = true
			name.TextColor3 = Color3.new(170,0,0)
			name.BackgroundTransparency = 1
			name.Size = UDim2.new(1,0,10,0)
			name.Font = Enum.Font.SourceSansLight
			name.TextSize = 20
			name.TextStrokeTransparency = .5

		end
	end

	Visual:addToggle("Auto ESP", false, function(value)
		if value == false then
			value = true
			autoESP = false
			if notifications == true then
				venyx:Notify("Auto ESP", "Off    ")
			end
		elseif value == true then
			value = false
			autoESP = true
			if notifications == true then
				venyx:Notify("Auto ESP", "On     ")
			end
		end
	end)

	Visual:addButton("Visual ESP", function()
		for i,v in pairs(Important.Players:GetChildren()) do
			CreateESP(v) 
		end
	end)

	function CreateHITBOXoff(plr)
		if plr ~= nil then
			local GetChar = plr.Character
			if not GetChar then return end

			local GetHead do
				repeat wait() until GetChar:FindFirstChild("HumanoidRootPart")
			end
			GetHead = GetChar.HumanoidRootPart     
			GetHead.Transparency = 1

		end
	end

	function CreateHITBOXon(plr)
		if plr ~= nil then
			local GetChar = plr.Character
			if not GetChar then return end

			local GetHead do
				repeat wait() until GetChar:FindFirstChild("HumanoidRootPart")
			end
			GetHead = GetChar.HumanoidRootPart     
			GetHead.Transparency = 0.7

		end
	end

	Visual:addButton("Visual Hitbox On", function()
		for i,v in pairs(Important.Players:GetChildren()) do
			CreateHITBOXon(v)
		end
	end)
	Visual:addButton("Visual Hitbox Off", function()
		for i,v in pairs(Important.Players:GetChildren()) do
			CreateHITBOXoff(v)
		end
	end)



