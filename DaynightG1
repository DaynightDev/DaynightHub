local autoESP = false
local autoHITBOX = false

local hitbox = playerTab:addSection("Extend Hitbox")
Important = {Players = game:GetService("Players"), Workspace = game:GetService("Workspace"), CoreGui = game:GetService("CoreGui")}
function CreateHITBOXEXoff(plr)
	if plr ~= nil then
		local GetChar = plr.Character
		if not GetChar then return end

		local GetHead do
			repeat wait() until GetChar:FindFirstChild("HumanoidRootPart")
		end
		GetHead = GetChar.HumanoidRootPart     
		GetHead.Size = Vector3.new(2, 2, 1)
	end
end

function CreateHITBOXEXon(plr)
	if plr ~= nil then
		local GetChar = plr.Character
		if not GetChar then return end

		local GetHead do
			repeat wait() until GetChar:FindFirstChild("HumanoidRootPart")
		end
		GetHead = GetChar.HumanoidRootPart
		GetHead.Size = Vector3.new(10, 10, 10)
	end
end

hitbox:addToggle("Auto Hitbox", false, function(value)
	if value == false then
		value = true
		autoHITBOX = false
		if notifications == true then
			venyx:Notify("Auto Hitbox", "Off    ")
		end
	elseif value == true then
		value = false
		autoHITBOX = true
		if notifications == true then
			venyx:Notify("Auto Hitbox", "On     ")
		end
	end
end)

hitbox:addButton("Extend Hitbox On", function()
	for i,v in pairs(Important.Players:GetChildren()) do
		CreateHITBOXEXon(v) 
			wait()
		game.Players.LocalPlayer.Character.HumanoidRootPart.Size = Vector3.new(2,2,1)
	end
end)

hitbox:addButton("Extend Hitbox Off", function()
	for i,v in pairs(Important.Players:GetChildren()) do
		CreateHITBOXEXoff(v) 
		wait()
		game.Players.LocalPlayer.Character.HumanoidRootPart.Size = Vector3.new(2,2,1)
	end
end)

local Visual = playerTab:addSection("Visual")
Important = {Players = game:GetService("Players"), Workspace = game:GetService("Workspace"), CoreGui = game:GetService("CoreGui")}
local enabledesp = false

function CreateESP(plr)
	if plr ~= nil then

		local GetChar = plr.Character
		if not GetChar then return end

		local GetHead do

			repeat wait() until GetChar:FindFirstChild("Head")

		end
		GetHead = GetChar.Head        

		local bb = Instance.new("BillboardGui", Important.CoreGui)
		bb.Adornee = GetHead
		bb.ExtentsOffset = Vector3.new(0, 1, 0)
		bb.AlwaysOnTop = true
		bb.Size = UDim2.new(0, 5, 0, 5)
		bb.StudsOffset = Vector3.new(0, 3, 0)
		bb.Name = "ESP_PLAYER_"

		local displayframe = Instance.new("Frame", bb)
		displayframe.ZIndex = 10
		displayframe.BackgroundTransparency = 1
		displayframe.Size = UDim2.new(1,0,1,0)

		local name = Instance.new("TextLabel", displayframe)
		name.Name = "Name"
		name.ZIndex = 10
		name.Text = plr.Name
		name.Visible = true
		name.TextColor3 = Color3.new(170,0,0)
		name.BackgroundTransparency = 1
		name.Size = UDim2.new(1,0,10,0)
		name.Font = Enum.Font.SourceSansLight
		name.TextSize = 20
		name.TextStrokeTransparency = .5

	end
end

Visual:addToggle("Auto ESP", false, function(value)
	if value == false then
		value = true
		autoESP = false
		if notifications == true then
			venyx:Notify("Auto ESP", "Off    ")
		end
	elseif value == true then
		value = false
		autoESP = true
		if notifications == true then
			venyx:Notify("Auto ESP", "On     ")
		end
	end
end)

Visual:addButton("Visual ESP", function()
	for i,v in pairs(Important.Players:GetChildren()) do
		CreateESP(v) 
	end
end)

function CreateHITBOXoff(plr)
	if plr ~= nil then
		local GetChar = plr.Character
		if not GetChar then return end

		local GetHead do
			repeat wait() until GetChar:FindFirstChild("HumanoidRootPart")
		end
		GetHead = GetChar.HumanoidRootPart     
		GetHead.Transparency = 1
		
	end
end

function CreateHITBOXon(plr)
	if plr ~= nil then
		local GetChar = plr.Character
		if not GetChar then return end

		local GetHead do
			repeat wait() until GetChar:FindFirstChild("HumanoidRootPart")
		end
		GetHead = GetChar.HumanoidRootPart     
		GetHead.Transparency = 0.7

	end
end

Visual:addButton("Visual Hitbox On", function()
	for i,v in pairs(Important.Players:GetChildren()) do
		CreateHITBOXon(v)
	end
end)
Visual:addButton("Visual Hitbox Off", function()
	for i,v in pairs(Important.Players:GetChildren()) do
		CreateHITBOXoff(v)
	end
end)



