local mapTab = venyx:addPage("Map", 5012544693)
local map4 = mapTab:addSection("Map Lighting")

map4:addButton("Remove Skies", function()
	if game.Lighting:FindFirstChild("Doom") then
		game.Lighting.Doom:Destroy()
		local S = game.ReplicatedStorage.Skies.Overworld.Shine:Clone()
		S.Parent = game.Lighting
	end
	if game.Lighting:FindFirstChild("OldShine") then
		game.Lighting.OldShine:Destroy()
		local S = game.ReplicatedStorage.Skies.Overworld.Shine:Clone()
		S.Parent = game.Lighting
	end
	if game.Lighting:FindFirstChild("Rain") then
		game.Lighting.Rain:Destroy()
		local S = game.ReplicatedStorage.Skies.Overworld.Shine:Clone()
		S.Parent = game.Lighting
	end
end)

map4:addButton("Shadows On", function()
	game.Lighting.GlobalShadows = true
end)

map4:addButton("Shadows Off", function()
	game.Lighting.GlobalShadows = false
end)

local map1 = mapTab:addSection("Crystal Bridge")

map1:addButton("Activate Crystal Bridge", function()
	game.Workspace.CrystalBridge.CanCollide = true
	game.Workspace.CrystalBridge.Transparency = 0
end)

map1:addButton("Deactivate Crystal Bridge", function()
	game.Workspace.CrystalBridge.CanCollide = false
	game.Workspace.CrystalBridge.Transparency = 1
end)

