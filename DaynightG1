local playerTab = venyx:addPage("Player", 5012544693)
local SpeedSL = playerTab:addSection("Speed")
local flySL = playerTab:addSection("Fly")
local autobreak = playerTab:addSection("AutoBreak")
local Hip = playerTab:addSection("HipHeight")
local FOV = playerTab:addSection("FieldOfView")

local plr = game:GetService("Players").LocalPlayer
local char = plr.Character
local mouse = game:GetService("Players").LocalPlayer:GetMouse()
local hum = char:FindFirstChild("HumanoidRootPart")
local loop = false
local loo3p = false
local dotoggle = false
local dotoggle2 = false
local dotoggle3 = false
local breaking = true
local did = false
local speed = 1
local norheight = 2
local backspeedlolidc = false

SpeedSL:addToggle("Toggle Speed", false, function(value)
	if value == false then
		value = true
		dotoggle = false
		if notifications == true then
			venyx:Notify("Speed", "Off    ")
		end
	elseif value == true then
		value = false
		dotoggle = true
		if notifications == true then
			venyx:Notify("Speed", "On     ")
		end
	end
end)

SpeedSL:addToggle("Toggle Backwards Speed", false, function(value)
	if value == false then
		value = true
		backspeedlolidc = false
		if notifications == true then
			venyx:Notify("Backwards Speed", "Off                ")
		end
	elseif value == true then
		value = false
		backspeedlolidc = true
		if notifications == true then
			venyx:Notify("Backwards Speed", "On                 ")
		end
	end
end)

SpeedSL:addSlider("Change Speed", 1, 0, 3, function(value)
	speed = value
end)

SpeedSL:addButton("See Your Speed", function()
	venyx:Notify("Speed", speed.."    ")
end)
flySL:addButton("Go on a Raft OR Activate Speed", function()
	
end)

flySL:addButton("Fly [E]", function()
	local speed55 = 0 
	local maxspeed = 50 
	repeat wait() 
	until game.Players.LocalPlayer and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:findFirstChild("Head") and game.Players.LocalPlayer.Character:findFirstChild("Humanoid") 
	local mouse = game.Players.LocalPlayer:GetMouse() 
	repeat wait() until mouse
	local plr = game.Players.LocalPlayer 
	local torso = plr.Character.Head 
	local flying = false
	local deb = true 
	local ctrl = {f = 0, b = 0, l = 0, r = 0} 
	local lastctrl = {f = 0, b = 0, l = 0, r = 0} 

	function Fly() 
		local bg = Instance.new("BodyGyro", torso) 
		bg.P = 9e4 
		bg.maxTorque = Vector3.new(9e9, 9e9, 9e9) 
		bg.cframe = torso.CFrame 
		local bv = Instance.new("BodyVelocity", torso) 
		bv.velocity = Vector3.new(0,0.1,0) 
		bv.maxForce = Vector3.new(9e9, 9e9, 9e9) 
		repeat wait() 
			plr.Character.Humanoid.PlatformStand = true 
			if ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0 then 
				speed55 = speed55+.5+(speed55/maxspeed) 
				if speed55 > maxspeed then 
					speed55 = maxspeed 
				end 
			elseif not (ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0) and speed55 ~= 0 then 
				speed55 = speed55-1 
				if speed55 < 0 then 
					speed55 = 0 
				end 
			end 
			if (ctrl.l + ctrl.r) ~= 0 or (ctrl.f + ctrl.b) ~= 0 then 
				bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f+ctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(ctrl.l+ctrl.r,(ctrl.f+ctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed55 
				lastctrl = {f = ctrl.f, b = ctrl.b, l = ctrl.l, r = ctrl.r} 
			elseif (ctrl.l + ctrl.r) == 0 and (ctrl.f + ctrl.b) == 0 and speed55 ~= 0 then 
				bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (lastctrl.f+lastctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(lastctrl.l+lastctrl.r,(lastctrl.f+lastctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed55 
			else 
				bv.velocity = Vector3.new(0,0.1,0) 
			end 
			bg.cframe = game.Workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(-math.rad((ctrl.f+ctrl.b)*50*speed55/maxspeed),0,0) 
		until not flying 
		ctrl = {f = 0, b = 0, l = 0, r = 0} 
		lastctrl = {f = 0, b = 0, l = 0, r = 0} 
		speed55 = 0 
		bg:Destroy() 
		bv:Destroy() 
		plr.Character.Humanoid.PlatformStand = false 
	end 
	mouse.KeyDown:connect(function(key) 
		if key:lower() == "e" then 
			if flying then flying = false 
			else 
				flying = true 
				Fly() 
			end 
		elseif key:lower() == "w" then 
			ctrl.f = 1 
		elseif key:lower() == "s" then 
			ctrl.b = -1 
		elseif key:lower() == "a" then 
			ctrl.l = -1 
		elseif key:lower() == "d" then 
			ctrl.r = 1 
		end 
	end) 
	mouse.KeyUp:connect(function(key) 
		if key:lower() == "w" then 
			ctrl.f = 0 
		elseif key:lower() == "s" then 
			ctrl.b = 0 
		elseif key:lower() == "a" then 
			ctrl.l = 0 
		elseif key:lower() == "d" then 
			ctrl.r = 0 
		end 
	end)
	Fly()
end)

local UIS = game:GetService("UserInputService")
UIS.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.W and dotoggle == true then
		if dotoggle == true then
			loo3p = true
			while loo3p do
				hum.CFrame = hum.CFrame + hum.CFrame.lookVector * speed
				wait()
			end
		end
	end
end)

UIS.InputEnded:Connect(function(output)
	if output.KeyCode == Enum.KeyCode.W then
		if dotoggle == true then
			loo3p = false
		end
	end
end)

local loopidc = false

UIS.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.S and dotoggle == true then
		if backspeedlolidc == true then
			loopidc = true
			while loopidc do
				hum.CFrame = hum.CFrame + hum.CFrame.lookVector * -1
				wait()
			end
		end
	end
end)

UIS.InputEnded:Connect(function(output)
	if output.KeyCode == Enum.KeyCode.S then
		if backspeedlolidc == true then
			loopidc = false
		end
	end
end)

autobreak:addToggle("Toggle Auto Break", false, function(value)
	if value == false then
		value = true
		dotoggle2 = true
		if notifications == true then
			venyx:Notify("Auto Break", "On     ")
		end
		breaking = false
	elseif value == true then
		value = false
		dotoggle2 = true
		if notifications == true then
			venyx:Notify("Auto Break", "Off      ")
		end
		breaking = true
		while breaking and wait(0.1) do
			local part = game:GetService("Players").LocalPlayer:GetMouse().Target
			local one = game:GetService("ReplicatedStorage").RelativeTime.Value
			local two = {part,part,part,part}
			game:GetService("ReplicatedStorage").Events.SwingTool:FireServer(one, two)
		end
	end
end)

Hip:addSlider("HipHeight", 2, 0, 100, function(value)
	game.Players.LocalPlayer.Character.Humanoid.HipHeight = value
	norheight = value
end)
Hip:addButton("Normal HipHeight", function()
	game.Players.LocalPlayer.Character.Humanoid.HipHeight = 2
end)

