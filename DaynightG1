		if FoodAmmount == 8 then ew3() ew3() ew3() ew3() ew3() ew3() ew3() ew3() end
		if FoodAmmount == 9 then ew3() ew3() ew3() ew3() ew3() ew3() ew3() ew3() ew3() end
		if FoodAmmount == 10 then ew3() ew3() ew3() ew3() ew3() ew3() ew3() ew3() ew3() ew3() end
	end
end, function()

end)

