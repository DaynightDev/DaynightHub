local settingsTab = venyx:addPage("Settings", 5012544693)
local setting = settingsTab:addSection("Settings")

setting:addButton("Destroy Gui", function()
	game.CoreGui["ScreenGui"].Enabled = true
	game.CoreGui["Booga Booga - Hybrid [Daynight Hub]"]:Destroy()
	dotoggle = false
	dotoggle2 = false
	dotoggle3 = false
	ToggleAutoEatTime = false
	autoHITBOX =false
	autoESP = false
end)

setting:addKeybind("Toggle Keybind", Enum.KeyCode.RightShift, function()
	venyx:toggle()
end, function()
end)

setting:addToggle("Toggle Notifications", false, function(value)
	if value == false then
		value = true
		notifications = true
		venyx:Notify("Notify", "On     ")
	elseif value == true then
		value = false
		notifications = false
	end
end)

while wait(1) do
	if ToggleAutoEatTime == true then
		wait(AutoEatTime)
		if FoodAmmount == 1 then ew3() end
		if FoodAmmount == 2 then ew3() ew3() end
		if FoodAmmount == 3 then ew3() ew3() ew3() end
		if FoodAmmount == 4 then ew3() ew3() ew3() ew3() end
		if FoodAmmount == 5 then ew3() ew3() ew3() ew3() ew3() end
		if FoodAmmount == 6 then ew3() ew3() ew3() ew3() ew3() ew3() end
		if FoodAmmount == 7 then ew3() ew3() ew3() ew3() ew3() ew3() ew3() end
		if FoodAmmount == 8 then ew3() ew3() ew3() ew3() ew3() ew3() ew3() ew3() end
		if FoodAmmount == 9 then ew3() ew3() ew3() ew3() ew3() ew3() ew3() ew3() ew3() end
		if FoodAmmount == 10 then ew3() ew3() ew3() ew3() ew3() ew3() ew3() ew3() ew3() ew3() end
	end
	
	if game.Workspace.Resources:FindFirstChild("Ancient Tree") then
		AchientTreeHealth = game.Workspace.Resources["Ancient Tree"].Health.Value
	end

	if game.Workspace.Resources:FindFirstChild("Wealthy God") then
		WealthyGodHealth = game.Workspace.Resources["Wealthy God"].Health.Value
	end

	if game.Workspace.Resources:FindFirstChild("Hateful God") then
		HatefulGodHealth = game.Workspace.Resources["Hateful God"].Health.Value
	end

	if game.Workspace.Resources:FindFirstChild("Lonely God") then
		LonelyGodHealth = game.Workspace.Resources["Lonely God"].Health.Value
	end

	if game.Workspace.Resources:FindFirstChild("Old God") then
		OldGodHealth = game.Workspace.Resources["Old God"].Health.Value
	end

	if game.Workspace.Resources:FindFirstChild("Miserable God") then
		MiserableGodHealth = game.Workspace.Resources["Miserable God"].Health.Value
	end

	if game.Workspace.Critters:FindFirstChild("Queen Penguin") then
		QueenPenguinHealth = game.Workspace.Critters["Queen Penguin"].Health.Value
	end

	if game.Workspace.Critters:FindFirstChild("Queen Ant") then
		QueenAntHealth = game.Workspace.Critters["Queen Ant"].Health.Value
	end

	if game.Workspace.Resources:FindFirstChild("Dancing Shelly") then
		DancingShellyHealth = game.Workspace.Resources["Dancing Shelly"].Health.Value
	end

	if game.Workspace.Critters:FindFirstChild("Shelby") then
		ShelbyHealth = game.Workspace.Critters["Shelby"].Health.Value
	end

	if game.Workspace.Critters:FindFirstChild("Shelbert") then
		ShelbertHealth = game.Workspace.Critters["Shelbert"].Health.Value
	end

	if game.Workspace.Critters:FindFirstChild("Sheldon") then
		SheldonHealth = game.Workspace.Critters["Sheldon"].Health.Value
	end
	
	if autoESP == true then
		wait(60)
		CreateESP(plr)
	end
	
	if autoHITBOX == true then
		wait(60)
		game.Players.LocalPlayer.Character.HumanoidRootPart.Size = Vector3.new(2,2,1)
		CreateHITBOXEXon(plr)
		wait()
		game.Players.LocalPlayer.Character.HumanoidRootPart.Size = Vector3.new(2,2,1)
		wait(0.1)
		game.Players.LocalPlayer.Character.HumanoidRootPart.Size = Vector3.new(2,2,1)
	end
end 
