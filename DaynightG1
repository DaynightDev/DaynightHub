	Event:FireServer("Void Greaves")
	wait(0.1)
	Event:FireServer("Void Chestplate")
	wait(0.1)
	Event:FireServer("Void Helmet")
	wait(0.1)
	if enableBag == true then
		Event:FireServer("Void Bag")
	end
	if notifications == true then
		venyx:Notify("Armor", "Equiped      ")
	end
end)

armorSL:addButton("God Armor", function()
	local Event = game: GetService ("ReplicatedStorage").Events.UseBagItem
	Event:FireServer("God Legs")
	wait(0.1)
	Event:FireServer("God Chestplate")
	wait(0.1)
	Event:FireServer("God Halo")
	wait(0.1)
	if enableBag == true then
		Event:FireServer("God Bag")
	end
	if notifications == true then
		venyx:Notify("Armor", "Equiped      ")
	end
end)

